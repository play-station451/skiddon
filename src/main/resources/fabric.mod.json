{
  "schemaVersion": 1,
  "id": "skiddon",
  "version": "${version}",
  "name": "Skiddon",
  "description": "An Skiddon for Meteor Client addons.",
  "authors": [
    "playstation451"
  ],
  "contact": {
    "sources": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.skid.addon.Skiddon"
    ]
  },
  "mixins": [
    "skiddon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.7", "1.21.6"],
    "meteor-client": "*"
  }
}
